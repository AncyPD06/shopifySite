<!-- product-template-new.liquid -->

<style>  
  .product-single__thumbnail:after, .img-border, .product__photo-wrapper, .lazypreload {
    border-radius: 10px;
  }
  .hero-image {
    width: calc(100% + 80px);
    margin-left: -40px;
    max-width: none;
  }
  .product-form__item--submit {
    margin-left: 0px !important;
  }
  .product-single__title {
    font-weight: 800;
    margin-bottom: 5px;
    font-size: 22px;
  }
  .price_properties .money {
    font-weight: 700;
    color: #424242;
  }
  .product-single__price {
    text-decoration: none;
    font-size: 18px;
  }
  .product-single__price--compare .money {
    color: #D9675C;
    font-size: 18px;
    text-decoration: line-through;
  }
  .photos__item .slick-arrow {
    background-color: #FCF8F4 !important;
    height: 26px;
    width: 26px;
  }
  .photos__item .slick-prev {
    left: -3px !important;
  }
  .photos__item .slick-next {
    right: -3px !important;
  }
   .photos__item .slick-prev:before {
    font-size: 15px !important;
  }
  .photos__item .slick-next:before {
   font-size: 15px !important;
  }
  .stamped-badge-caption {
    text-decoration: underline !important;
  }
  span.stamped-badge-caption:before {
    content: "(";
  }
  .stamped-badge-caption span:after {
    content: ")";
  }
  .short-description {
    color: #424242;
    font-size: 14px;
  }
  .pb-3 {
    margin-top: 20px;
    padding-bottom: 0 !important;
  }
  .stock-text {
    font-size: 10px;
  }
  .label-properties {
    font-weight: 900;
    color: #4C4B4D;
  }
  .single-option label{
    border-radius: 0.5rem;
    background-color: transparent;
    border: 0.5px solid #707070;
  }
  .single-option input[type=radio]:checked + label {
    border: 2.5px solid #424242 !important;
  }
  .product-accordion {
    margin-top: 1.5rem;
  }
  .product-accordion .card {
    border-bottom: 1px solid #e9e9e9;
  }
  .product-accordion .border-top {
    border-top: 1px solid #e9e9e9;
    padding-right: 0 !important;
  }
  .product-accordion .card-header {
    border-bottom: 0 !important;
    padding: 14px 0 14px 0 !important;
  }
  .product-accordion .card-body {
    color: #424242;
    font-size: 12px;
    padding-top: 0;
  }
  .r-arrow {
    right: 0;
    top: 1.75em;
    position: absolute;
  }
  .card-link {
    font-weight: 700;
    font-size: 14px;
    text-transform: uppercase;
  }
 
  .product-single__photo {
      margin-bottom: 36px;
  }
  .template-product .main-content{
    padding-top:40px;
  }
  #productrelated {
    margin: 35px 0px;
  }
  .pswp__button--arrow--left:before {
    content: url(../assets/arrow.svg);
    background: none;
  }
  .product-single__thumbnails .slick-next:before {
    height: 1rem;
    content: '' !important;
    margin: 0 auto;
    background-image: url({{ 'arrow.svg' | asset_url }});
    background-repeat: no-repeat;
    background-position: center center;
    display: block;
    width: 1rem;
  }
  .product-single__thumbnails .slick-prev:before {
    height: 1rem;
    content: '' !important;
    margin: 0 auto;
    background-image: url({{ 'arrow.svg' | asset_url }});
    background-repeat: no-repeat;
    background-position: center center;
    display: block;
    width: 1rem;
    transform: rotate(180deg);
  }
  .pswp__button--arrow--left:before {
      background: #FCF8F4;
      background-image: url({{ 'arrow.svg' | asset_url }}) !important;
      background-repeat: no-repeat;
      background-position: center center;
      border-radius: 1rem;
      transform: rotate(180deg);
  }
  .pswp__button--arrow--right:before {
      background: #FCF8F4;
      background-image: url({{ 'arrow.svg' | asset_url }}) !important;
      background-repeat: no-repeat;
      background-position: center center;
      border-radius: 1rem;
  }
  .prime-button {
        margin-top: 20px;
  }
  .card-body a {
    text-decoration: underline;
    color: #424241; 
  }
  #ContactFormEmail {
    border-radius: 2rem; 
    padding-left: 2.25rem;
  }
  .modal-content {
    border-radius: 30px !important;
  }
  .close {
    top: 2rem !important;
    right: 2rem !important;
  }
  .js-notify-text span{
    color: #424242 !important;
  }
  .js-contact-soldout {
    margin-top: 20px;
  }
  sup {
    top: -1.2em !important;
  }
  sup .product-label {
    display: flex;
  }
  sup .product-tag {
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 12px !important;
  }


  

/* TABLET */
  @media (min-width:767px) {
    .hero-image {
      width: calc(100% + 40px);
      margin-left: -20px;
      max-width: none;
    }
    .product-single__title {
      font-size: 25px;
    }
    .short-description {
      font-size: 16px;
    }
    .product-single__price {
      font-size: 22px;
    }
    .product-single__price--compare .money {
      font-size: 22px;
    }
    .stock-text {
      font-size: 14px;
    }
    .card-link {
      font-size: 15px;
    }
    .product-accordion .card-header {
      padding: 16px 0 16px 0 !important;
    }
  }




/* DESKTOP */
  @media (min-width:992px) {
    .product-single__title {
      font-size: 30px;
    }
    .photos__item .slick-arrow {
      height: 46px;
      width: 46px;
    }
    .photos__item .slick-prev {
      left: -10px !important;
    }
    .photos__item .slick-next {
      right: -10px !important;
    }
    .photos__item .slick-prev:before {
      font-size: 18px !important;
    }
    .photos__item .slick-next:before {
       font-size: 18px !important;
    }
    .product-single__photo {
      margin-bottom: 20px;
    }
    .template-product .main-content{
      padding-top:80px;
    }
    .hero-image {
      width: calc(100% + 372px);
      margin-left: -186px;
      max-width: none;
    }
    #productrelated {
      margin: 60px 0px;
    }
    .short-description {
      font-size: 18px;
    }
    .product-single__price {
      font-size: 24px;
    }
    .product-single__price--compare .money {
      font-size: 24px;
    }
    .card-link {
      font-size: 16px;
    }
    .product-accordion .card-header {
      padding: 18px 0 18px 0 !important;
    }
    .product-accordion .card-body {
      font-size: 14px;
    }
    .product-single__thumbnails .slick-next:before {
        height: 2rem;
        background-image: url({{ 'thumbnail-arrow.svg' | asset_url }});
    }
    .product-single__thumbnails .slick-prev:before {
        height: 2rem;
        background-image: url({{ 'thumbnail-arrow.svg' | asset_url }});
    }
  }
  



  

  
  .modal-backdrop.show{
    display:none !important;
  }
  .product-modal__image {
    display: block;
    position: relative;
    top: 50%;
    -ms-transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    display: block;
    max-height: 95%;
    max-width: 95%;
    margin: 0 auto;
    cursor: auto
  }
  .no-csstransforms .product-modal__image {
    top: 2.5%
  }
  #nav-tab-info .nav-tabs {
    border-bottom: none;
    text-align: center;
    justify-content: center;
  }
  #nav-tab-info .nav-tabs .nav-link {
    border: none;
    border-bottom: 2px solid transparent;
    color: #222;
    font-size: 16px;
  }
  #nav-tab-info .nav-tabs .nav-link.active {
    border-bottom: 2px solid {{settings.color_primary}};
  }
  #nav-tab-info .nav-tabs .nav-item {
    padding:0;
    margin:0 5rem -2px 0;
    font-weight: 600;
    background-color: transparent;
    font-size: calc(var(--g-font-size) + 2px);
    padding:5px 0;
  }
  .nav-link .spr-badge-starrating {
    display: none
  }
  .social-sharing .col-title {
    font-size: 14px;
    line-height: 1.8;
    min-width: 75px;
  }
  .safe-checkout-detail {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
  }
  .product-form__quantity-submit {
    display: flex;
    align-items: center;
    padding-top: 10px;
  }
  .product-form__quantity-submit .product-form__item--submit {
    margin-left: 10px;
  }
  .selector-wrapper {
    margin: 15px 0;
  }
  #shopify-section-{{ section.id }} .product-single__info-wrapper{
      padding:0rem 0rem 0rem 3rem;
  }
  .product-form-select .selector-wrapper{
    flex: 0 0 50%;
    max-width: 50%;
  }
  @media (max-width:1100px) {
    #shopify-section-{{ section.id }} .product-single__info-wrapper{
      padding:0rem 0rem 0rem 0rem;
    }
    .product-form__quantity-submit{
      display: block;
    }
    .product-form__item--quantity{
      width: 130px;
      margin-bottom: 10px;
    }
    .product-form__quantity-submit .product-form__item--submit{
      margin:0 0 10px 0 !important;
    }
    .product-form__buynow{
      margin:0;
    }
  }
  @media (max-width:767px) {
    .main-content {
      padding-top: 20px;
    }
  }
</style>


<div class=" js-recently-view" itemscope itemtype="http://schema.org/Product" id="ProductSection-{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="product" data-enable-history-state="true" data-ajax="true" data-stock="{{ section.settings.stock_enable }}" data-handle="{{product.handle}}">
  {% assign product_image_size = '480x480' %}
  {% assign current_variant = product.selected_or_first_available_variant %}
  <meta itemprop="name" content="{{ product.title }}">
  <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
  <meta itemprop="image" content="{{ product.featured_image.src | img_url: '600x600' }}">
  <meta itemprop="brand" content="{{ product.vendor }}">
  <div id="2" class="container-fluid">
    <div class="row product-single ">
<!-- PRODUCT IMAGE -->
      <div class="col-md-6">
        <div class="photos">
<!-- BIG IMAGE -->
          <div class="photos__item photos__item--main " >
            <div class="my-gallery" itemscope itemtype="http://schema.org/ImageGallery">
              {%- assign featured_image = current_variant.featured_image | default: product.featured_image -%}
              {%- for image in product.images -%}
              <figure  itemprop="associatedMedia" itemscope itemtype="http://schema.org/ImageObject" class="product-single__photo product__photo-container product__photo-container-{{ section.id }}{% unless image == featured_image %} hide{% endunless %}"
                  id="ProductPhoto{{ section.id }}"
                  style="max-width: {% include 'image-width' with image: image, width: 480 %}px;"
                  data-image-id="{{ image.id }}">
                  <a href="{{ image | img_url: '1400x' }}" itemprop="contentUrl" data-size="1400x1400"
                  class="product__photo-wrapper product__photo-wrapper-{{ section.id }} zoom_image zoom zoom_enabled"
                  style="padding-top:{{ 1 | divided_by: image.aspect_ratio | times: 100}}%;">
                  {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                  <img itemprop="thumbnail" class="lazyload {% unless image == featured_image %} lazypreload{% endunless %}"
                      src="{{ image | img_url: '1200x' }}"
                      data-src="{{ img_url }}"
                      data-widths="[180, 240, 360, 480, 720, 960, 1080, 1296, 1512, 1728, 2048]"
                      data-aspectratio="{{ image.aspect_ratio }}"
                      data-sizes="auto"
                      alt="{{ image.alt | escape }}">
                  </a>
              </figure>
              {%- endfor -%}
            </div>
            <noscript>
              <a href="{{ featured_image | img_url: '1200x' }}">
                <img src="{{ featured_image | img_url: product_image_size }}" alt="{{ featured_image.alt | escape }}" id="ProductPhotoImg-{{ section.id }}">
              </a>
            </noscript>
            {% include 'productVideo' %}
          </div>
<!-- THUMBNAIL IMAGES -->
          {%- if product.images.size > 1 -%}
          <div class="photos__item photos__item--thumbs">
            <div class="product-single__thumbnails product-single__thumbnails-{{ section.id }} product-single__thumbnails--static">
              {%- for image in product.images -%}
              <div class="product-single__thumbnail-item product-single__thumbnail-item-{{ section.id }}{% if image == featured_image %} is-active{% endif %}" data-image-id="{{ image.id }}">
                <a href="{{ image.src | img_url: product_image_size }}" data-zoom="{{ image.src | img_url: '1200x' }}" class="product-single__thumbnail product-single__thumbnail-{{ section.id }}">
                  {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                  <img class="img-border lazyload {% unless image == featured_image %} lazypreload{% endunless %}"
                      src="{{ image | img_url: '1200x' }}"
                      data-src="{{ img_url }}"
                      data-widths="[180, 240, 360, 480, 720, 960, 1080, 1296, 1512, 1728, 2048]"
                      data-aspectratio="{{ image.aspect_ratio }}"
                      data-sizes="auto"
                      alt="{{ image.alt | escape }}">
                </a>
              </div>
              {%- endfor -%}
            </div>
          </div>
          {%- endif -%}
        </div>
      </div>
<!-- PRODUCT DESCRIPTION -->
      <div class="col-12 col-md-6 order-1" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
        <div class="product-single__info-wrapper">
          <meta itemprop="priceCurrency" content="{{ shop.currency }}">
          <link itemprop="availability" href="http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}">
          <div class="product-single__meta small--text-center">
<!-- PRODUCT TITLE -->
          <h1 itemprop="name" class="product-single__title">{{ product.title }}<sup>
              {% include 'product-label' %}
           </sup>
          </h1>
<!-- AFFILIATED PRODUCTS PAYMENT BUTTON -->
          {% if product.metafields.info.affiliate_link != blank %}{% comment %}AFFILIATE PRODUCT{% endcomment %}
            <a href="{{product.metafields.info.affiliate_link}}" 
               class="my-3 btn btn-theme btn--full product-form__cart-submit 
               {% if section.settings.enable_payment_button %}product-form__cart-submit--outline{% endif %}">
                      {{product.metafields.info.affiliate_button}}
            </a>
<!-- NORMAL PRODUCTS FORM CREATION -->
          {% else %} {% comment %}NORMAL PRODUCT{% endcomment %}
            {% capture "form_class" %}product-form {% if section.settings.product_selector == 'select'%} product-form-select{% endif %} {% if section.settings.enable_payment_button %} product-form--payment-button{% endif %}{%- endcapture %}
            {% capture "form_id" %}AddToCartForm-{{ section.id }}{%- endcapture %}
            {% form 'product', product, class:form_class, id:form_id %}
              {% comment %}
                Automatically inserted by Shop Pay Installments theme updater
              {% endcomment %}
<!-- STAR REVIEW -->
            {% if product.metafields.stamped.reviews_count %}
            <span class="stamped-product-reviews-badge stamped-main-badge" 
              data-id="{{ product.id }}" 
              data-product-sku="{{ product.handle }}" 
              data-product-title="{{product.title}}" 
              data-product-type="{{product.type}}" 
              style="display: block;">
                {{product.metafields.stamped.badge}}
            </span>
            {% endif %}
<!-- SHORT DESCRIPTION -->
            {% if product.metafields.custom.product_short_description != '' %}
              <p class="short-description">{{ product.metafields.custom.product_short_description }}</p>
            {% endif %}
<!-- TO SHOW PRODUCT VARIANTS 1 -->
            {% unless product.has_only_default_variant %}
            {% for option in product.options_with_values %}
            <div class="selector-wrapper js product-form__item">
              <label class="label-properties" {% if option.name == 'default' %}class="label--hidden" {% endif %}for="SingleOptionSelector-{{ section.id }}-{{ forloop.index0 }}">{{ option.name }}</label>
              {% include 'productOption' %}
            </div>
            {% endfor %}
            {% endunless %}
<!-- TO SHOW PRODUCT VARIANTS 2 -->
            <select name="id" id="ProductSelect-{{ section.id }}" class="product-form__variants no-js">
              {% for variant in product.variants %}
              <option {% if variant == current_variant %} selected="selected" {% endif %} data-sku="{{ variant.sku }}" value="{{ variant.id }}" {% unless variant.available %} disabled="disabled" {% endunless %}>
                {% if variant.available %}
                  {{ variant.title }} - {{ variant.price | money_with_currency }}
                {% else %}
                  {{ variant.title }} - {{ 'products.product.sold_out' | t }}
                {% endif %}
              </option>
              {% endfor %}
            </select>
<!-- PRODUCT PRICE -->
            <ul class="pb-3 product-single__meta-list list--inline{% if shop.taxes_included or shop.shipping_policy.body != blank %} product-single__price-container{% endif %}">
              <li>
                {%- unless product.compare_at_price_max > product.price -%}
                  <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                {%- endunless -%}
                <span id="ProductPrice-{{ section.id }}" class="price product-single__price price_properties" itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}">
                  {{ current_variant.price | money }}
                </span>
              </li>
              {% if product.compare_at_price_max > product.price %}
              <li>
                <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                <s id="ComparePrice-{{ section.id }}" class="price product-single__price product-single__price--compare">
                  {{ current_variant.compare_at_price | money }}
                </s>
              </li>
              {% endif %}
              {%- if section.settings.stock_enable -%}
              <li>{%- include 'productStock' -%}</li>
              {%- endif -%}
            </ul>
<!-- IN STOCK TEXT -->
            {% if product.available %}
              <p class="stock-text">{{ section.settings.in_stock_text }}</p>
            {% endif %}
<!-- ADD TO CART BUTTON -->
            <div class="product-form__item product-form__item--submit {% unless section.settings.enable_payment_button %} btn-disablebuynow {% endunless %}">
              <button type="submit"
                name="add"
                id="AddToCart-{{ section.id }}"
                class="btn button btn--full btn-theme product-form__cart-submit {% if section.settings.enable_payment_button %}product-form__cart-submit--outline{% endif %}{% unless current_variant.available %} btn--sold-out{% endunless %}"
                {% unless current_variant.available %}disabled="disabled"{% endunless %}>
                  <span class="button-text js-grid-cart"
                    title="{{ 'products.product.add_to_cart' | t }}" data-id="{{product.selected_or_first_available_variant.id}}"
                    id="AddToCartText-{{ section.id }}">
                    {% unless current_variant.available %}
                    {{ 'products.product.sold_out' | t }}
                    {% else %}
                    {{ 'products.product.add_to_cart' | t }}
                    {% endunless %}
                  </span>
                </button> 
              </div>
<!-- Beginning of Buy With Prime Widget -->
              {% if current_variant.available %}
              <div class="prime-button" style="padding: 5px;">&nbsp</div>
              <script defer src="https://code.buywithprime.amazon.com/default.variant.selector.shopify.js"></script>
              {% if product.sku %}
                <div
                  id="amzn-buy-now"
                  data-site-id="l6dhp821f8"
                  data-widget-id="w-1JM0jMyRyd4mvPkWDEkZX3"
                  data-sku="{{product.sku}}"
                ></div>
              {% else %}
                <div
                  id="amzn-buy-now"
                  data-site-id="l6dhp821f8"
                  data-widget-id="w-1JM0jMyRyd4mvPkWDEkZX3"
                  data-sku="{{product.selected_or_first_available_variant.sku}}"
                ></div>
              {% endif %}
                {% endif %}
<!-- End of Buy With Prime Widget -->
<!-- HURRY BUY -->
              {%- if section.settings.enable_hurrify -%}
              <span class="js-hurrify {% if current_variant.inventory_management %}{% if current_variant.inventory_quantity >= 10 or current_variant.inventory_quantity <= 0 %}hide{% endif %}{% else %}hide{% endif %}">
                <p class="text-danger font-weight-bold">hurry buy this!</p>
                <div class="progress">
                  <div class="progress-bar" style="width:{{current_variant.inventory_quantity | times: 10}}%">
                  </div>
                </div>
              </span>
              {%- endif -%}
<!-- JOIN WAITLIST BUTTON -->
              <div class="js-contact-soldout {% if current_variant.available %}hide{% endif %}">
                <span class="btn btn-waitlist btn-theme" data-toggle="modal" data-target="#jsSoldout" title="{{ 'products.product.soldout_messenger' | t }}">{{'products.product.waitlist' | t}}</span>
              </div>
            {% endform %}
<!-- JOIN WAITLIST BUTTON CLICK DIALOGUE -->
            {% include 'notifySoldoutProduct' %}
          {% endif %}
        </div>
<!-- DESCRIPTION - bottom -->
        {% if section.settings.positiontab == "bottom" %}
        <div class="section-desc" style="background:{{section.settings.bg_tab}};padding:{{section.settings.ptab}};margin:padding:{{section.settings.mtab}}">
          <div class="container ">
            <div class="row justify-content-md-center">
              <div class="{{section.settings.coldes}} col-12">
                {% if section.settings.enable_tabvertical %}
                  {%- include 'product-information-vertical-new' -%}
                {% else %}
                  {%- include 'product-information' -%}
                {% endif %}
              </div>
            </div>
          </div>
        </div> 
        {% endif %}
<!-- DESCRIPTION - right -->
        {% if section.settings.positiontab == "right" %}
          {% if section.settings.enable_tabvertical %}
            {%- include 'product-information-vertical-new' -%}
          {% else %}
            {%- include 'product-information' -%}
          {% endif %}
       {% endif%}
<!-- DESCRIPTION - left -->
        {% if section.settings.positiontab == "left" %}
            {% if section.settings.enable_tabvertical %}
              {%- include 'product-information-vertical-new' -%}
            {% else %}
              {%- include 'product-information' -%}
            {% endif %}
       {% endif %}
      </div>
    </div>
  </div>
    
<!-- TO ADD BLOCKS FOR THIS SECTION -->
      {% for block in section.blocks %}
        {% if block.type == 'html' %}
          {%- include 'html-product' -%}
        {% elsif block.type == 'video' %}
          {%- include 'video-product'-%}
        {% elsif block.type == 'related-product' %}
          {%- include 'perfect-pairings' -%}
        {% elsif block.type == 'service' %}
          {%- include 'service-pro' -%}
        {% elsif block.type == 'hero' %}
          {%- include 'hero-image-video' -%}
        {% elsif block.type == 'service-callout' %}
          {% include 'service-callout' %}
        {% elsif block.type == 'feature-callout-1' %}
          {% include 'feature-callout', pfc_title: product.metafields.custom.feature_callout_title_1, pfc_image: product.metafields.custom.feature_callout_image_1, pfc_description: product.metafields.custom.feature_callout_details_1 %}
        {% elsif block.type == 'feature-callout-2' %}
          {% include 'feature-callout', pfc_title: product.metafields.custom.feature_callout_title_2, pfc_image: product.metafields.custom.feature_callout_image_2, pfc_description: product.metafields.custom.feature_callout_details_2 %}
        {% elsif block.type == 'media-callout' %}
          {% include 'media-callout' %}
        {% elsif block.type == 'feature-slideshow' %}
          {%- include 'feature-slideshow' -%}
